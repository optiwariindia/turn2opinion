<!doctype html>
<html lang="en">
	{% include "dbcomp/head.twig" %}
	<body>
		<div class="app-container app-theme-white body-tabs-shadow fixed-header fixed-sidebar">
			{% include "dbcomp/header.twig" %}
			<div class="app-main">
				{% include "dbcomp/sidebar.twig" %}
				<div class="app-main__outer">
					<div class="app-main__inner">
						{% include "dbcomp/title.twig" %}
						{% include "dbcomp/tabs.twig" %}
						<div class="tabs-animation">
							<div class="row">
								<div class="col-sm-12 col-lg-4">
									{% include "dbcomp/profileStatus.twig" %}
									{% include "dbcomp/graph.twig" %}
								</div>
								<div class="col-sm-12 col-lg-8">

									{% include "dbcomp/prostat.twig" %}
								</div>
							</div>
						</div>
						<div class="mb-3"></div>
						<div class="row">
							<div class="col-sm-12">{% include "dbcomp/surveys.twig" %}</div>
						</div>
					</div>

				</div>
			</div>
		</div>
		<div class="app-drawer-overlay d-none animated fadeIn"></div>
		{% if (user.terms |length)==0  %}
			<div class="forced-popup">
				<div class="popup-body">
				{{user.terms|json_encode}}
					{% include "dbcomp/terms.twig" %}
				</div>
			</div>
		{% endif %}
		<script src="/dashboard.js"></script>
		<script type="text/javascript" src="/assets/scripts/main.d810cf0ae7f39f28f336.js"></script>
	</body>
<script type="text/javascript">
var Tawk_API=Tawk_API||{}, Tawk_LoadStart=new Date();
(function(){
var s1=document.createElement("script"),s0=document.getElementsByTagName("script")[0];
s1.async=true;
s1.src='https://embed.tawk.to/6235adc6a34c2456412bce27/1fugqtkbu';
s1.charset='UTF-8';
s1.setAttribute('crossorigin','*');
s0.parentNode.insertBefore(s1,s0);
})();
</script>
</html>
