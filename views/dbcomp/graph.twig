<div class="mb-3 card">
	<div class="card-header-tab card-header">
		<div class="card-header-title font-size-lg text-capitalize font-weight-normal">
			{# <i class="header-icon lnr-cloud-download icon-gradient bg-happy-itmeo"></i> #}
			<img src="/img/earning.png" style='margin-right:10px; height:35px'  class='img-fluid' alt="" srcset="">
			Earnings Curve
		</div>
	</div>
	<div class="p-0 card-body">
		<div class="p-1 slick-slider-sm mx-auto">
			<div class="slick-slider">
				<div>
					<div class="widget-chart widget-chart2 text-left p-0">
						<div class="widget-chat-wrapper-outer">
							<div class="widget-chart-wrapper he-auto opacity-10 m-0">
								<canvas id="spark-earnings"></canvas>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<p class="text-muted text-uppercase font-size-md opacity-5 pl-3 pr-3 pb-1 font-weight-normal">Weeks</h6>
		<ul class="list-group list-group-flush">
			<li class="p-3 bg-transparent list-group-item">
				<div class="widget-content p-0">
					<div class="widget-content-outer">
						<div class="widget-content-wrapper">
							<div class="widget-content-left">
								<div class="widget-heading">Total Earnings (This Month)</div>
								{# <div class="widget-subheading">Last year expenses</div> #}
							</div>
							<div class="widget-content-right">
								<div class="widget-numbers text-success">
									<small>$</small>
									<span data='totalearnings'></span>
								</div>
							</div>
						</div>
						<div class="widget-progress-wrapper">
							<div class="progress-bar-sm progress-bar-animated-alt progress">
								<div class="progress-bar bg-primary" role="progressbar" aria-valuenow="43" aria-valuemin="0" aria-valuemax="100" style="width: 0%;"></div>
							</div>
						</div>
					</div>
				</div>
			</li>
		</ul>
	</div>
</div>
