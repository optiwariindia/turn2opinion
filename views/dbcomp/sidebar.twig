<div class="app-sidebar sidebar-shadow">
	<div class="app-header__logo">
		<div class="logo-src"></div>
		<div class="header__pane ml-auto">
			<div>
				<button type="button" class="hamburger close-sidebar-btn hamburger--elastic" data-class="closed-sidebar">
					<span class="hamburger-box">
						<span class="hamburger-inner"></span>
					</span>
				</button>
			</div>
		</div>
	</div>
	<div class="app-header__mobile-menu">
		<div>
			<button type="button" class="hamburger hamburger--elastic mobile-toggle-nav">
				<span class="hamburger-box">
					<span class="hamburger-inner"></span>
				</span>
			</button>
		</div>
	</div>
	<div class="app-header__menu">
		<span>
			<button type="button" class="btn-icon btn-icon-only btn btn-primary btn-sm mobile-toggle-header-nav">
				<span class="btn-icon-wrapper">
					<i class="fa fa-ellipsis-v fa-w-6"></i>
				</span>
			</button>
		</span>
	</div>
	<div class="scrollbar-sidebar">
		<div class="app-sidebar__inner">
			<ul class="vertical-nav-menu">
				<li {% if page.title == "Dashboard" %}class=mm-active{% endif %}>
					<a href="/user/dashboard"  {% if page.title == "Dashboard" %}class=mm-active{% endif %}>
						<i class="metismenu-icon pe-7s-rocket"></i>Dashboard
					</a>
				</li>
				<li {% if page.title == "Profile" %}class=mm-active{% endif %}>
					<a href="/user/profile" {% if page.title == "Profile" %}class=mm-active{% endif %} >
					<img class="sidebar-icons" src="/images/categories/beauty-profile.png" alt="" srcset=""	<h5 class="metismenu-icon ">My Profile</h5></a>
				</li>
				<li class="app-sidebar__heading">Survey Profiling Categories</li>
				{% for category in user.profileCategories %}
					<li {% if category.active == true %}class=mm-active{% endif %}>
						<a href="/user/profile/{{category.target}}"  {% if category.active == true %}class=mm-active{% endif %} >
							<img class='sidebar-icons' src="/images/categories/{{category.icon}}" alt="" srcset="">
							{{ category.name }} Profile
						</a>
					</li>
				{% endfor %}
			</ul>
		</div>
	</div>
</div>
